{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://clubmanager-pro-server.up.railway.app/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}